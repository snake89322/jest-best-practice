{
  "name": "jest-best-practice",
  "version": "1.0.0",
  "description": "jest pratice",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "precommit": "lint-staged"
  },
  "standard": {
    "ignore": [
      "__test__/"
    ]
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "standard --fix",
        "git add"
      ]
    },
    "ignore": [
      "**/__test__/**/*"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "npm run precommit"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snake89322/jest-best-practice.git"
  },
  "keywords": [
    "jest",
    "pracitice",
    "javascript"
  ],
  "author": "Cary Chen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/snake89322/jest-best-practice/issues"
  },
  "homepage": "https://github.com/snake89322/jest-best-practice#readme",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@commitlint/cli": "^8.0.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.0",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "dayjs": "^1.8.14",
    "node-fetch": "^2.6.0"
  }
}
