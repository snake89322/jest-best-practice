{
  "name": "jest-best-practice",
  "version": "1.0.0",
  "description": "jest pratice",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "precommit": "lint-staged"
  },
  "standard": {
    "ignore": [
      "__test__/",
      "__mocks__/"
    ],
    "parser": "babel-eslint"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "standard --fix",
        "git add"
      ]
    },
    "ignore": [
      "**/__test__/**/*"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "npm run precommit"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snake89322/jest-best-practice.git"
  },
  "keywords": [
    "jest",
    "pracitice",
    "javascript"
  ],
  "author": "Cary Chen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/snake89322/jest-best-practice/issues"
  },
  "homepage": "https://github.com/snake89322/jest-best-practice#readme",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.0.0",
    "babel-eslint": "^10.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.0.1",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "jscheckstyle": "0.0.9",
    "lint-staged": "^8.2.0",
    "sino": "0.0.3",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "dayjs": "^1.8.14",
    "node-fetch": "^3.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
